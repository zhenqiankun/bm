<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
  <display-name>Archetype Created Web Application</display-name>
  <listener>
    <listener-class>Listen.Listen</listener-class>
  </listener>
  <listener>
    <listener-class>Listen.RequestListen</listener-class>
  </listener>
  <listener>
    <listener-class>Listen.SessionLIsten</listener-class>
  </listener>
  <jsp-config>
    <taglib>
      <taglib-uri>/mytag</taglib-uri>
      <taglib-location>/WEB-INF/tlds/MyTaglib.tld</taglib-location>
    </taglib>
  </jsp-config>
  <servlet>
    <servlet-name>infor</servlet-name>
    <servlet-class>Server.StudentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>infor</servlet-name>
    <url-pattern>/infor</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>up</servlet-name>
    <servlet-class>Server.SServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>up</servlet-name>
    <url-pattern>/up</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
  </welcome-file-list>
</web-app>